all:
	mvn compile

logs:
	cd .. && docker compose logs -f backend

reload:
	@echo "ðŸ”„ Recompiling and restarting..."
	cd .. && docker compose exec backend mvn compile
	@echo "âœ… Done!"

restart:
	cd .. && docker compose restart backend
	@echo "âœ… Backend restarted"

status:
	cd .. && docker compose ps backend
